# Uncomment the next line to define a global platform for your project
 platform :ios, '12.0'

target 'Speedway Motors iOS Framework' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  #use_frameworks!
source 'https://github.com/CocoaPods/Specs.git'


  # Pods for Speedway Motors iOS Framework
pod 'ZDCChat'
pod 'ZendeskSDK', '2.3.0'
pod 'ZendeskSDK/Providers'
pod 'GoogleMaps'
pod 'GooglePlaces', '3.0.3'
pod 'ImageSlideshow'
pod 'SwiftyJSON', '4.2'



  target 'Speedway Motors iOS FrameworkTests' do
    inherit! :search_paths
    # Pods for testing
    pod 'GoogleMaps'
    pod 'GooglePlaces', '3.0.3'
  end

  target 'Speedway Motors iOS FrameworkUITests' do
    inherit! :search_paths
    # Pods for testing
    pod 'GoogleMaps'
    pod 'GooglePlaces', '3.0.3'

  end

end
